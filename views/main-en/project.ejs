<%- include('includes/head') %>

    <!-- breadcrumb start-->
    <section class="breadcrumb breadcrumb_bg">
        <div class="container">
            <div class="row">
                <div class="col-lg-12">
                    <div class="breadcrumb_iner text-center">
                        <div class="breadcrumb_iner_item">
                            <h2>Projects</h2>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </section>
    <!-- breadcrumb start-->

    <!-- project_part part start-->
    <section class="project_part padding_bottom padding_top">
        <div class="container">
            <div class="row justify-content-between">
                <div class="col-lg-4">
                    <div class="section_tittle">
                        <h2>our projects</h2>
                    </div>
                </div>
            </div>
            <div class="row">
                <%projects.forEach(p=>{ %>
                    <div class="col-lg-3">
                        <div class="card" style="width: 100%">
                            <img src="<%=p.img%>" class="card-img-top" alt="...">
                            <div class="card-body">
                                <h5 class="card-title">
                                    <%=p.name.en%>
                                </h5>
                                <p class="card-text">
                                    <%=p.desc.en.slice(0,50)%>
                                </p>
                                <a href="/project/<%=p._id%>" class="btn_1">learn more <span><img
                                            src="/main/img/icon/left.svg" alt=""></span>
                                </a>
                            </div>
                        </div>
                    </div>
                    <%})%>
            </div>
        </div>
    </section>
    <!-- project_part part end-->
    <!-- map us part start-->
    <section class="map_part">
        <div class="container-fluid">
            <div class="row">
                <div class="col-lg-12">
                    <div class="map_iner">
                        <div class="map"></div>
                    </div>
                </div>
            </div>
        </div>
    </section>
    <!-- map us part end-->
    <%- include('includes/footer') %>