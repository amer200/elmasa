        </div>
    </div>
    <!-- our clients part end-->

    <!-- about part start-->
    <div class="about_part section_bg">
        <div class="container-fluid">
            <div class="row justify-content-end">
                <div class="col-lg-5 col-md-6">
                    <div class="about_part_text" style="text-align: right;">
                        <h2>من نحن</h2>
                        <p>

                            <%=about.ar.slice(0,200)%>
                        </p>
                        <a href="/about" class="btn_1">المزيد<span><img src="/main/img/icon/left.svg" alt=""></span>
                        </a>
                    </div>
                </div>
                <div class="col-lg-7 col-md-6">
                    <div class="about_img">
                        <img src="<%=about.img%>" alt="#">
                    </div>
                </div>
            </div>
        </div>
    </div>
    <!-- about part end-->


    <!-- service part start-->
    <section class="service_part">
        <div class="container">
            <div class="row">
                <div class="col-lg-6">
                    <div class="section_tittle">
                        <h2>خدماتنا</h2>
                    </div>
                </div>
            </div>
            <div class="row">
                <div class="col-lg-12">
                    <div class="service_slider owl-carousel">
                        <% servs.forEach(s=>{ %>
                            <div class="single_service_slide">
                                <div class="row justify-content-end align-items-center single_service">
                                    <div class="col-lg-6 col-md-6">
                                        <img src="<%=s.img%>" alt="#">
                                    </div>
                                    <div class="col-lg-5 col-md-6">
                                        <div class="service_text" style="text-align: right;">
                                            <h3>
                                                <%=s.title.ar%>
                                            </h3>
                                            <p>
                                                <%=s.content.ar%>
                                            </p>
                                        </div>
                                    </div>
                                </div>
                            </div>
                            <%})%>
                    </div>
                </div>
            </div>
        </div>
    </section>
    <!-- service part end-->

    <!-- project_part part start-->
    <section class="project_part padding_bottom">
        <div class="container">
            <div class="row justify-content-between">
                <div class="col-lg-4">
                    <div class="section_tittle">
                        <h2>مشاريعنا</h2>
                    </div>
                </div>
            </div>
            <div class="row">
                <div class="col-lg-12">
                    <div class="project_slider owl-carousel">
                        <% projectsP.forEach(p=>{%>
                            <div class="single_project_slide">
                                <div class="row">
                                    <% p.forEach(p=>{ %>
                                        <div class="col-lg-4 col-md-6">
                                            <img src="<%=p.imgs[0]%>" alt="#">
                                            <div class="single_project_text" style="text-align: right;">

                                                <p>
                                                    <%=p.desc.ar.slice(0,50) %>...
                                                </p>
                                                <a href="/project/<%=p._id%>" class="btn_1">المزيد<span><img
                                                            src="/main/img/icon/left.svg" alt=""></span> </a>
                                            </div>
                                        </div>
                                        <%})%>
                                </div>
                            </div>
                            <%})%>
                    </div>
                </div>
            </div>
        </div>
    </section>
    <!-- project_part part end-->
