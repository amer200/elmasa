<%- include('includes/head') %>
    <!-- banner part start-->
    <section class="banner_part">
        <div class="container">
            <div class="row">
                <div class="col-lg-6">
                    <div class="banner_text">
                        <% projects.forEach(p=>{ %>
                            <div class="banner_text_iner">
                                <h5>
                                    <%=p.categ.ar%>
                                </h5>
                                <h1>
                                    <%=p.name.ar%>
                                </h1>
                                <p>
                                    <%= p.desc.ar.slice(0,30)%> ...
                                </p>
                                <div class="banner_btn">
                                    <a href="/project/<%=p._id%>" class="btn_1">learn more <span><img
                                                src="/main/img/icon/left.svg" alt=""></span>
                                    </a>
                                </div>
                            </div>
                            <%})%>
                    </div>
                    <div class="nav next"><a href="#"><span class="flaticon-left-arrow"></span></a></div>
                    <div class="nav prev"><a href="#"><span class="flaticon-right-arrow"></span></a></div>
                </div>
            </div>
        </div>
    </section>
    <div class="our_speciality">
        <section class="single_page_special">
            <div class="container">
                <div class="row">
                    <div class="col-lg-12">
                        <div class="clients_images_top">
                            <img src="/main/img/clints/massa-5.png" alt="">
                            <img src="/main/img/clints/massa-6.png" alt="">
                            <img src="/main/img/clints/massa-7.png" alt="">
                        </div>
                    </div>
                </div>
            </div>
        </section>
    </div>
    <!-- banner part start-->


