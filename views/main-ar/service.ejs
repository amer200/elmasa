<%- include('includes/head') %>
    <!-- breadcrumb start-->
    <section class="breadcrumb breadcrumb_bg">
        <div class="container">
            <div class="row">
                <div class="col-lg-12">
                    <div class="breadcrumb_iner text-center">
                        <div class="breadcrumb_iner_item">
                            <h2>خدماتنا</h2>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </section>
    <!-- breadcrumb start-->

    <!-- service part start-->
    <section class="service_part">
        <div class="container">
            <div class="row">
                <div class="col-lg-6">
                    <div class="section_tittle">
                        <h2>خدماتنا</h2>
                    </div>
                </div>
            </div>
            <div class="row">
                <div class="col-lg-12">
                    <div class="service_slider owl-carousel">
                        <%servs.forEach(s=>{ %>
                            <div class="single_service_slide">
                                <div class="row justify-content-end align-items-center single_service">
                                    <div class="col-lg-6 col-md-6">
                                        <img src="<%=s.img%>" alt="#">
                                    </div>
                                    <div class="col-lg-5 col-md-6">
                                        <div class="service_text" style="text-align: right;">
                                            <h3>
                                                <%=s.title.ar%>
                                            </h3>
                                            <p>
                                                <%=s.content.ar%>
                                            </p>
                                        </div>
                                    </div>
                                </div>
                            </div>
                            <%})%>
                    </div>
                </div>
            </div>
        </div>
    </section>
    <!-- service part end-->

    <!-- contact us part start-->
    <!-- map us part start-->
    <section class="map_part">
        <div class="container-fluid">
            <div class="row">
                <div class="col-lg-12">
                    <div class="map_iner">
                        <div class="map"></div>
                    </div>
                </div>
            </div>
        </div>
    </section>
    <!-- map us part end-->

    <%- include('includes/footer') %>